		"@mui/system": "^5.15.14",
		"@mui/x-date-pickers": "^7.0.0",
		"@wordpress/api-fetch": "^6.50.0",
		"@wordpress/compose": "^6.30.0",
		"@wordpress/element": "^5.30.0",
		"@wordpress/scripts": "^27.3.0",
		"@wpackio/entrypoint": "^6.4.0",
		"@wpackio/scripts": "^6.4.0",
		"dayjs": "^1.11.10",
		"react-hot-loader": "^4.13.1",
		"sass": "^1.42.1",
		"url-loader": "^4.1.1"
